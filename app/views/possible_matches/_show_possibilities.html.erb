<span id = "possible_matches" ng-controller = 'PossibleMatches'>
	<%= render partial: 'wardrobes/_update_wardrobe', locals: {possible_matches}%>
	<span class = 'possible_pieces_for_suggested_bottom' ng-if = "suggested_piece.merch_type == bottom">
		<span class = 'possible_pieces_top'>
			<%= render partial: '_possible_pieces_contemplated_piece', locals: {possible_matches}%>
		</span>
		<span class = "possible_pieces_bottom" ng-switch = 'suggested_piece.id'>		
			<%= render partial: '_possible_pieces_suggested_piece', locals: {possible_matches}%>
		</span>
	</span>
	<span class = 'possible_pieces_for_suggested_top' ng-if = "suggested_piece.merch_top == top">
		<span class = "possible_pieces_top">
			<%= render partial: '_possible_pieces_suggested_piece', locals: {possible_matches}%>
		</span>
		<span class = "possible_pieces_bottom">
			<%= render partial: '_possible_pieces_contemplated_piece', locals: {possible_matches} %>
		</span>
	</span>
</span>
